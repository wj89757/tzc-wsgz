{__NOLAYOUT__}
<html lang="en">
<head>
  <meta charset="UTF-8">
  <!-- 分别为：屏幕宽度和设备一直，初始缩放比例，最大缩放比例和禁止用户缩放 -->
  <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
  <title>台院电商实验室-数据导出</title>
  <!-- jQuery -->
  <load href="__PUBLIC__/js/jQuery.js" />
  <!-- bootstrap.min.javascript -->
  <load href="__PUBLIC__/js/bootstrap.min.js" />
  <!-- style CSS -->
  <load href="__PUBLIC__/css/back.css" />
  <!-- Bootstrap CSS -->
  <load href="__PUBLIC__/css/bootstrap.min.css" />
</head>
<body>
  <!-- 头部文件开始 -->
  <include file="Public/header" />
  <!-- 头部文件结束 -->
  <!--自创sideba-->
    <div class="container" style="min-height: 500px;padding:0;margin:0;">
      <div class="row">
        <!-- 个人中心侧边栏开始 -->
        <include file="Public/backside" />
        <!-- 个人中心侧边栏结束 -->
        <div class="col-md-9 col-sm-8 col-lg-9 col-xs-8">
          <div class="main">
              <div class="main-h" >
                
                <ul class="breadcrumb">
                  <li>
                    <a href={:U('Personal/index')}>个人中心</a> <span class="divider"></span>
                  </li>
                  <li>
                    <a href={:U('Personal/download')}>数据导出</a> <span class="divider"></span>
                  </li>
                  <li class="active">
                    主页
                  </li>
                </ul>
                <h3 class="side-lined" style="color:#000">{$username}欢迎你！</h3>
                <li class="list-group-item">
                  <a href={:U('Personal/expUser')}>
                    <span class="glyphicon glyphicon-plus"></span>
                    &nbsp;导出数据并生成excel
                  </a>
                </li>
                <form action="{:U('Personal/impUser')}" method="post" enctype="multipart/form-data">
                    <!-- <input type="file" name="import"/> -->
                <input type="hidden" name="table" value="tablename"/>
                </form>
              </div>
            </div>
        </div>
      </div>
    </div>
    <div class="clearfix"></div>
    <!-- 底部文件footer开始 -->
    <include file="Public/footer" />
    <!-- 底部文件footer结束 -->
</body>
</html>






  