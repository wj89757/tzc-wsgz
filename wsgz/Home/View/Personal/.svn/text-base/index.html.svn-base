{__NOLAYOUT__}
<html lang="en">
<head>
  <meta charset="UTF-8">
  <!-- 分别为：屏幕宽度和设备一直，初始缩放比例，最大缩放比例和禁止用户缩放 -->
  <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
  <title>治水官网后台管理-首页</title>
  <!-- jQuery -->
  <load href="__PUBLIC__/js/jquery.min.js" />
  <!-- bootstrap.min.javascript -->
  <load href="__PUBLIC__/js/bootstrap.min.js" />
  <!-- style CSS -->
  <load href="__PUBLIC__/css/back.css" />
  
  <!-- Bootstrap CSS -->
  <load href="__PUBLIC__/css/bootstrap.min.css" />
  <!-- style CSS -->
  <load href="__PUBLIC__/css/style.css" />

</head>
<body>
  <!-- 头部文件开始 -->
  <include file="Public/nav" />
  <!-- 头部文件结束 -->
  <!--自创sideba-->
    <div class="container" style="min-height: 500px;padding:0;margin:0;">
      <div class="row">
        <!-- 个人中心侧边栏开始 -->
        <include file="Public/backside" />
        <!-- 个人中心侧边栏结束 -->
        <div class="col-md-9 col-sm-8 col-lg-9 col-xs-8">
          <div class="main">
              <div class="main-h" >
                  <ul class="breadcrumb">
                  <li>
                    <a href={:U('Personal/index')}>个人中心</a> <span class="divider"></span>
                  </li>
                  <li class="active">
                    主页
                  </li>
                </ul>
                  <h3 class="side-lined" style="color:#000">{$username}欢迎你！</h3>
                  <div class="container-fluid" style="padding:0;padding-bottom: 10px;">
                    <div class="row-fluid">
                      <div class="span9">
                        <div class="carousel slide" id="carousel-929811">
                          <ol class="carousel-indicators">
                            <li class="active" data-slide-to="0" data-target="#carousel-929811">
                            </li>
                            <li data-slide-to="1" data-target="#carousel-929811">
                            </li>
                            <li data-slide-to="2" data-target="#carousel-929811">
                            </li>
                          </ol>
                          <div class="carousel-inner">
                            <div class="item active">
                              <img alt="" src="__PUBLIC__/img/bg2.png" />
                              
                            </div>
                            <div class="item">
                              <img alt="" src="__PUBLIC__/img/bg1.png" />
                              
                            </div>
                            <div class="item">
                              <img alt="" src="__PUBLIC__/img/bg3.png" />
                              
                            </div>
                          </div>
                          <a href="#carousel-929811" data-slide="prev" class="carousel-control left">
                            <span class="glyphicon glyphicon-chevron-left"></span>
                          </a>
                          <a href="#carousel-929811" data-slide="next" class="carousel-control right">
                            <span class="glyphicon glyphicon-chevron-right"></span>
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
              </div>
            </div>
        </div>
      </div>
    </div>

    <div class="clearfix"></div>
    <!-- 底部文件footer开始 -->
    <include file="Public/footer" />
    <!-- 底部文件footer结束 -->
</body>
</html>



